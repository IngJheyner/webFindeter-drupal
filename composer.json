{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/addtoany": "^1.14",
        "drupal/admin_audit_trail": "^1.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_theme": "^1.6",
        "drupal/advagg": "^4.1",
        "drupal/antibot": "^1.5",
        "drupal/auditfiles": "^2.11",
        "drupal/bootstrap4": "^2.1",
        "drupal/bootstrap_layouts": "^5.2",
        "drupal/calendar": "^1.0@alpha",
        "drupal/captcha": "^1.1",
        "drupal/colorbox": "^1.7",
        "drupal/config_split": "^1.7",
        "drupal/console": "^1.0.2",
        "drupal/content_access": "^1.0@alpha",
        "drupal/core-composer-scaffold": "9.4.4",
        "drupal/core-project-message": "9.4.4",
        "drupal/core-recommended": "9.4.4",
        "drupal/cshs": "^3.0",
        "drupal/custom_search": "^1.0@beta",
        "drupal/devel": "^4.1",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/easy_install": "^10.4",
        "drupal/editor_file": "^1.5",
        "drupal/encrypt": "^3.0",
        "drupal/entity_class_formatter": "^1.3",
        "drupal/entity_type_clone": "^1.3",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/facets": "^1.5",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_formatter_class": "^1.4",
        "drupal/field_group": "^3.1",
        "drupal/field_permissions": "^1.1",
        "drupal/file_browser": "^1.3",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/fontawesome": "^2.17",
        "drupal/google_analytics": "^3.1",
        "drupal/honeypot": "^2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/mailchimp": "^1.11",
        "drupal/menu_block": "^1.6",
        "drupal/menu_per_role": "^1.2",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/migrate_tools": "^5.0",
        "drupal/module_filter": "^3.2",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.6",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/potx": "^1.0@alpha",
        "drupal/quick_node_clone": "^1.13",
        "drupal/readonlymode": "^1.1",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/redis": "1.5",
        "drupal/search_api": "^1.17",
        "drupal/search_autocomplete": "^2.0",
        "drupal/seckit": "^2.0",
        "drupal/select2": "^1.13",
        "drupal/simple_sitemap": "^3.10",
        "drupal/simplify": "^1.2",
        "drupal/slick": "^2.2",
        "drupal/slick_extras": "^1.0@RC",
        "drupal/slick_views": "^2.3",
        "drupal/smtp": "^1.0",
        "drupal/sqlsrv": "^4.0",
        "drupal/structure_sync": "^2.0",
        "drupal/superfish": "^1.4",
        "drupal/svg_image": "^1.14",
        "drupal/taxonomy_manager": "^2.0.3",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/taxonomy_unique": "^2.3",
        "drupal/twig_tweak": "^2.8",
        "drupal/upgrade_status": "^3.13",
        "drupal/video_embed_field": "^2.4",
        "drupal/video_embed_html5": "^1.2",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_data_export": "^1.0",
        "drupal/views_fieldsets": "^3.3",
        "drupal/views_templates": "^1.1",
        "drupal/webform": "^6.0@alpha",
        "drupal/xls_serialization": "^1.2",
        "drush/drush": "^9.7.1 | 10.3.6",
        "kint-php/kint": "^3.3",
        "mikehaertl/phpwkhtmltopdf": "^2.4",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "predis/predis": "^2.0",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "wikimedia/composer-merge-plugin": "^2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches": {
            "drupal/core": {
                "Notice: Undefined index: value in Drupal\\views\\Plugin\\views\\filter\\NumericFilter->acceptExposedInput()": "https://www.drupal.org/files/issues/2020-06-04/2825860-exposed-filter-notice-38.patch",
                "Exposed date filter leads to a notice": "https://www.drupal.org/files/issues/2020-11-12/2811887-53.patch",
                "The entity:user context is required and not present.": "https://www.drupal.org/files/issues/2019-10-23/3056234-10.patch",
                "Call to a member function getCacheMaxAge() on null in getCacheMaxAge()": "https://www.drupal.org/files/issues/2020-09-08/get_cache_tags_null-3169694-2.patch",
                "TypeError: Argument 1 passed to Drupal/views/Plugin/views/field/EntityOperations::getEntityTranslation() must implement interface Drupal/Core/Entity/EntityInterface, null given": "https://www.drupal.org/files/issues/2021-01-21/3105003-9.patch",
                "Multiple usages of FieldPluginBase::getEntity do not check for NULL, leading to WSOD": "https://www.drupal.org/files/issues/2021-01-06/3007424-108.patch"
            },
            "drupal/content_access": {
                "content access role": "https://www.drupal.org/files/issues/2020-08-28/content_access-role-2782813-25.patch"
            },
            "drupal/sqlsrv": {
                "Call to a member function fetchField() on null": "https://www.drupal.org/files/issues/2022-06-18/sqlsrv-undefined-key-return-3291199-4.patch"
            },
            "drupal/redis": {
                "Default lifetime for permanent TTL": "https://www.drupal.org/files/issues/2020-09-23/redis-default_permanent_ttl-2944938.patch"
            }
        }
    }
}
