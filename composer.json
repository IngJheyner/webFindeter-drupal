{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.7",
        "drupal/addtoany": "^1.14",
        "drupal/admin_toolbar": "^2.3",
        "drupal/adminimal_theme": "^1.6",
        "drupal/backup_migrate": "^4.1",
        "drupal/bootstrap4": "^2.1",
        "drupal/bootstrap_layouts": "^5.2",
        "drupal/captcha": "^1.1",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_split": "^1.5",
        "drupal/console": "^1.0.2",
        "drupal/content_sync": "^2.1",
        "drupal/core": "^8.8.0",
        "drupal/core-composer-scaffold": "^8.8.0",
        "drupal/cshs": "^1.2",
        "drupal/devel": "2.1",
        "drupal/devel_php": "1.2",
        "drupal/disable_field": "^2.4",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/entity_class_formatter": "^1.3",
        "drupal/entity_type_clone": "^1.3",
        "drupal/facets": "^1.5",
        "drupal/field_formatter_class": "^1.4",
        "drupal/field_group": "^3.1",
        "drupal/fontyourface": "^3.3",
        "drupal/mailchimp": "^1.11",
        "drupal/menu_block": "^1.6",
        "drupal/migrate_plus": "4.2",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/migrate_tools": "4.5",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.6",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/quick_node_clone": "^1.13",
        "drupal/search_api": "^1.17",
        "drupal/search_kint": "^1.0",
        "drupal/slick": "2.2",
        "drupal/slick_extras": "^1.0@RC",
        "drupal/slick_views": "2.3",
        "drupal/sqlsrv": "3.1.3",
        "drupal/structure_sync": "^2.0",
        "drupal/taxonomy_manager": "2.0.3",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/taxonomy_unique": "^2.3",
        "drupal/twig_tweak": "^2.8",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_fieldsets": "^3.3",
        "drupal/webform": "^5.22",
        "drupal/wincachedrupal": "^8.0",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "mikehaertl/phpwkhtmltopdf": "^2.4",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "wikimedia/composer-merge-plugin": "^1.4",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8.0",
        "kint-php/kint": "^3.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "patches":{
            "drupal/core": {
                "Notice: Undefined index: value in Drupal\\views\\Plugin\\views\\filter\\NumericFilter->acceptExposedInput()":"https://www.drupal.org/files/issues/2020-06-04/2825860-exposed-filter-notice-38.patch",
                "Multiple usages of FieldPluginBase::getEntity do not check for NULL, leading to WSOD": "https://www.drupal.org/files/issues/2020-09-03/3007424-108.patch",
                "AssertionError: Tokens need to be valid Twig variables. in assert":"https://www.drupal.org/files/issues/2019-05-22/views-ignore_invalid_tokens_in_replace-3056489-1-D8.6.16.patch",
		"Exposed date filter leads to a notice" : "https://www.drupal.org/files/issues/2020-11-12/2811887-53.patch"
            }
        }
    }
    
}
